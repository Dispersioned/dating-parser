{
  "name": "VK BETTER DATING",
  "description": "С лёгкостью получите АПИ ключи для клиента VK Better Dating",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://vk.com/dating*"],
      "js": ["foreground.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://vk.com/*"],
      "resources": ["/js/injected.js"]
    }
  ],
  "permissions": [],
  "host_permissions": ["https://vk.com/dating?ref=catalog_recent"],
  "icons": {
    "16": "./icons/logo16.png",
    "32": "./icons/logo32.png",
    "48": "./icons/logo64.png",
    "128": "./icons/logo128.png"
  }
}
